{
  "version": 2,
  "name": "cobotium-website",
  "builds": [
    { "src": "**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/docs", "dest": "/docs/index.html" },
    { "src": "/docs/(.*)", "dest": "/docs/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
